    "laravel/framework": "^5.4|^6.0|^7.0",
    "jurager/exchange1c": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "orchestra/testbench": "^5.1",
    "mockery/mockery": "^1.3"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jurager\\Exchange\\ExchangeServiceProvider"
      ]
    }
  }
