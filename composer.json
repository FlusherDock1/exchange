{
  "name": "jurager/exchange",
  "type": "library",
  "description": "Catalog Loader From 1C - CommerceML with Laravel",
  "keywords": [
    "1C",
    "CommerceML",
    "Laravel"
  ],
  "homepage": "https://github.com/jurager",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Kartsev",
      "role": "Developer"
    },
    {
      "name": "Yuri Gerassimov",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
